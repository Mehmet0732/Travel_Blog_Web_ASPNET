@using Travel_Web_Page.Models.Siniflar
@model List<Blog>
@{
    Layout = null;
}

<div class="padding1">
    <h4>En Beyendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogImage" style="width:120px; height:65px;" alt="" />
            </div>
            <div class="bot-desc">
                <p>@x.Aciklama.Substring(0,50)... </p>
            </div>
            <div class="clear"></div>
        }



    </div>
   @Html.Action("Partial5","Default")
</div>